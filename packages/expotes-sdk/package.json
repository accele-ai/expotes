{
  "name": "@expotes/sdk",
  "version": "0.0.1",
  "description": "Expotes SDK library generated by Nestia",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "./structures": {
      "import": {
        "types": "./dist/es/structures/index.d.ts",
        "default": "./dist/es/structures/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/structures/index.d.cts",
        "default": "./dist/cjs/structures/index.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "npm run build && node copylib.js",
    "build": "npm run build:sdk && npm run structures && npm run compile",
    "build:sdk": "rimraf src/functional && rimraf src/structures && cd ../expotes-api && npm run sdk",
    "compile": "rimraf dist && bunchee",
    "deploy": "npm run build && npm publish",
    "prepare": "ts-patch install && typia patch",
    "structures": "ctix build --mode create -p ./tsconfig.json -o ./src/structures"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@nestia/sdk": "^3.8.0",
    "bunchee": "^5.3.2",
    "ctix": "^2.6.1",
    "rimraf": "^6.0.1",
    "ts-patch": "^3.2.1",
    "tsup": "^8.2.3",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@nestia/fetcher": "^3.8.0",
    "typia": "^6.5.4"
  }
}
